{
  "name": "signalor-static-server",
  "version": "1.0.0",
  "description": "S3-compatible bucket file server with GUI and proxy",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "dev": "bun run src/server.ts",
    "build": "bun run --bun tsc",
    "start": "bun dist/server.js",
    "type-check": "bun run --bun tsc --noEmit"
  },
  "keywords": [
    "s3",
    "bucket",
    "file-server",
    "proxy"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.425.0",
    "@aws-sdk/lib-storage": "^3.425.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express-rate-limit": "^7.1.0",
    "redis": "^4.6.10",
    "mime-types": "^2.1.35",
    "multer": "^1.4.5-lts.1",
    "cookie-parser": "^1.4.6",
    "rate-limit-redis": "^4.1.5"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "tsx": "^4.7.0",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "@types/mime-types": "^2.1.2",
    "@types/multer": "^1.4.7",
    "@types/cookie-parser": "^1.4.4",
    "@types/cors": "^2.8.17"
  }
}
